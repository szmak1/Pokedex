{
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "parser": "babylon",
    "printWidth": 80,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "strict",
    "endOfLine": "lf",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "parser": "json"
            }
        },
        {
            "files": "*.md",
            "options": {
                "parser": "markdown"
            }
        },
        {
            "files": "*.yml",
            "options": {
                "parser": "yaml"
            }
        },
        {
            "files": "*.yaml",
            "options": {
                "parser": "yaml"
            }
        }
    ]
}
